{
  "settings_popup": {
    "type": "array",
    "margin": "0, 0",
    "content": [
      {
        "type": "group",
        "header": "Autotracking",
        "header_background": "#3e4b57",
        "content": [
          { "type": "item", "item": "opt_auto_map_switch" }
        ]
      }
    ]
  },
  "tracker_full": {
    "type": "container",
    "background": "#000000",
