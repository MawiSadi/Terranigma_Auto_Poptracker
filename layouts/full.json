          },
          "content": {
            "type": "layout",
            "key": "shared_item_grid"
          }
        },
        {
          "type": "group",
          "header": "Map",
          "content": {
            "type": "mapimage",
            "image_key": "current_map",
            "images_path": "images/maps/",
